{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "userId",
    "amount",
    "accountFrom",
    "accountTo"
  ],
  "properties": {
    "userId": {
      "type": "string",
      "maxLength": 26,
      "minLength": 26
    },
    "amount": {
      "type": "integer",
      "minimum": 1
    },
    "accountFrom": {
      "type": "string",
      "maxLength": 8,
      "minLength": 8
    },
    "accountTo": {
      "type": "string",
      "maxLength": 8,
      "minLength": 8
    }
  }
}
